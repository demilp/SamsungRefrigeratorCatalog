{
  "name": "catalog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "contentful-vue": "^1.0.5",
    "jquery": "^3.3.1",
    "vue": "^2.5.17",
    "vue-carousel": "^0.16.0-rc1",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vue-slick": "^1.1.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^4.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-cond-assign": "off",
      "no-undef": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "cover 99.5%"
  ]
}